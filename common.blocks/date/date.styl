.date {
  span {
    font-size 40px;
    line-height 30px;
  }
  color #ffffff;
  width: 206px;
  background url('data:image/png;base64,') no-repeat left center;
  padding: 28px 20px 21px 38px;
  margin-top: 30px;
  &__title {
    display inline-block;
    text-align center;
    padding-right: 5px;
  }

  &__month {
    display: inline-block;
    padding-bottom: 17px;
    vertical-align: middle;
  }

}