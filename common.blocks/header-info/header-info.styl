.header-info {
  float right;
  padding-top: 7px;

  &__telephone {
    display block;
    text-decoration none;
    float left;
    font-size 18px;
    background url('data:image/png;base64,') no-repeat left center;
    padding: 11px 0 14px 86px;
    color #706f6f;
  }

  &__feedback {
    display block;
    float left;
    border 2px solid #895996;
    padding 9px 10px 13px 10px;
    color #520676;
    font-size 18px;
    margin-left 30px;
    text-decoration none;
  }
}