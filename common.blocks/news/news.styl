.news {
  width: 264px;
  height: 213px;
  display inline-block;
  background url('data:image/png;base64,') no-repeat left top;
  margin-top: 38px;
  margin-left: 10px;

  &__title {
    color #6d367c;
    font-size 18px;
    text-decoration underline;
    text-align: center;
    margin: 0;
    padding-top: 13px;
  }

  &__date {
    font-size 14px;
    line-height 26px;
    text-align center;
    color #ffffff;
    padding-top: 13px;
  }

  &__item-title {
    color #ffffff;
    font-size 18px;
    padding-left: 27px;
    display: block;
  }

  &__desc {
    color #ffffff;
    font-size 14px;
    width: 213px;
    padding-left: 27px;
    padding-top: 5px;
  }

  .owl-theme .owl-nav.disabled + .owl-dots {
    margin-top: 0px;
  }

  .owl-theme .owl-dots .owl-dot span {
    background: #c284b9;
    width: 12px;
    height: 12px;
    margin: 5px 5px;
  }
  .owl-theme .owl-dots .owl-dot.active span, .owl-theme .owl-dots .owl-dot:hover span {
    background: #faf1d6;
  }
}

.news__carousel {
  width 100%;
  overflow hidden;
}

